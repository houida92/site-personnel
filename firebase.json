{
  "firebase": "site-personnel",
  "public": "dist/site-personnel",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [
    {
      "source": "/public/**",
      "destination": "/public.html"
    },
    {
      "source": "**",
      "destination": "/site-personnel/index.html"
    }
  ]
 
}
